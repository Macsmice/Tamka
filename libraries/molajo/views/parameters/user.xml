<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id: contact.xml $ -->
<form>
	<fields name="params">
        <fieldset
            name="profile"
			label="MOLAJO_CONFIG_ITEM_LAYOUT_PARAMETER_CATEGORIES"
		    >
			<field
				name="address1"
				type="text"
				id="address1"
				description="JGLOBAL_PROFILE_FIELD_ADDRESS1_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_ADDRESS1_LABEL"
				message="JGLOBAL_PROFILE_FIELD_ADDRESS1_MESSAGE"
				size="30"
			/>

			<field
				name="address2"
				type="text"
				id="address2"
				description="JGLOBAL_PROFILE_FIELD_ADDRESS2_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_ADDRESS2_LABEL"
				message="JGLOBAL_PROFILE_FIELD_ADDRESS1_MESSAGE"
				size="30"
			/>

			<field
				name="city"
				type="text"
				id="city"
				description="JGLOBAL_PROFILE_FIELD_CITY_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_CITY_LABEL"
				message="JGLOBAL_PROFILE_FIELD_CITY_MESSAGE"
				size="30"
			/>

			<field
				name="region"
				type="text"
				id="region"
				description="JGLOBAL_PROFILE_FIELD_REGION_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_REGION_LABEL"
				message="JGLOBAL_PROFILE_FIELD_REGION_MESSAGE"
				size="30"
			/>

			<field
				name="country"
				type="text"
				id="country"
				description="JGLOBAL_PROFILE_FIELD_COUNTRY_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_COUNTRY_LABEL"
				message="JGLOBAL_PROFILE_FIELD_COUNTRY_MESSAGE"
				size="30"
			/>

			<field
				name="postal_code"
				type="text"
				id="postal_code"
				description="JGLOBAL_PROFILE_FIELD_POSTAL_CODE_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_POSTAL_CODE_LABEL"
				message="JGLOBAL_PROFILE_FIELD_POSTAL_CODE_MESSAGE"
				size="30"
			/>

			<field
				name="phone"
				type="tel"
				id="phone"
				description="JGLOBAL_PROFILE_FIELD_PHONE_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_PHONE_LABEL"
				message="JGLOBAL_PROFILE_FIELD_PHONE_MESSAGE"
				size="30"
			/>

			<field
				name="website"
				type="url"
				id="website"
				description="JGLOBAL_PROFILE_FIELD_WEB_SITE_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_WEB_SITE_LABEL"
				message="JGLOBAL_PROFILE_FIELD_WEB_SITE_MESSAGE"
				size="30"
			/>
			<field
				name="favoritebook"
				type="text"
				description="JGLOBAL_PROFILE_FIELD_FAVORITE_BOOK_DESC"
				filter="string"
				label="JGLOBAL_PROFILE_FIELD_FAVORITE_BOOK_LABEL"
				message="JGLOBAL_PROFILE_FIELD_FAVORITE_BOOK_MESSAGE"
				size="30"
			/>
			<field
				name="aboutme"
				type="textarea"
				description="JGLOBAL_PROFILE_FIELD_ABOUT_ME_DESC"
				label="JGLOBAL_PROFILE_FIELD_ABOUT_ME_LABEL"
				message="JGLOBAL_PROFILE_FIELD_ABOUT_ME_MESSAGE"
				cols="30"
				rows="5"
				filter="safehtml"
			/>
			<field
					name="tos"
					type="radio"
					label="JGLOBAL_PROFILE_FIELD_TOS_LABEL"
					description="JGLOBAL_PROFILE_FIELD_TOS_DESC">
					<option	value="1">JGLOBAL_PROFILE_OPTION_AGREE</option>
			</field>
			<field
					name="dob"
					type="calendar"
					label="JGLOBAL_PROFILE_FIELD_DOB_LABEL"
					description="JGLOBAL_PROFILE_FIELD_DOB_DESC"
				 />
		</fieldset>
    </fields>
</form>